<header>
  <ul>
    <h1><%= link_to "Members Only", root_path %></h1>
    <% if :current_user.nil? %>
    <li><%= link_to "Log In", login_path %></li>
    <li><%= link_to "Sign up now!", signup_path %></li>
    <% else %>
    <li><%= link_to "Log Out", logout_path, method: :delete %></li>
    <% end %>
  </ul>
</header>